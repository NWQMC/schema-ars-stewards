<?xml version="1.0" encoding="UTF-8"?> 
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.2.xsd">

    <preConditions>
          <dbms type="oracle" />
          <runningAs username="ARS_STEWARDS" />
    </preConditions>

    <changeSet author="drsteini" id="loadCharNameToType" runOnChange="true">
        <delete tableName="ars_char_name_to_type"/>
        <loadData encoding="UTF-8"
                  file="changeLogData/char_name_to_type.csv"
                  tableName="char_name_to_type">
        </loadData>
        <rollback>select 'no rollback - you must restore previous csv and rerun manually' from dual</rollback>
    </changeSet>

    <changeSet author="drsteini" id="loadSiteTypeToPrimary" runOnChange="true">
        <delete tableName="site_type_to_primary"/>
        <loadData encoding="UTF-8"
                  file="changeLogData/site_type_to_primary.csv"
                  tableName="site_type_to_primary">
        </loadData>
        <rollback>select 'no rollback - you must restore previous csv and rerun manually' from dual</rollback>
    </changeSet>

</databaseChangeLog>